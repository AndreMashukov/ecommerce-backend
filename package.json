{
  "name": "ecommerce-backend",
  "version": "3.2.0",
  "description": "A delightful way to building a Node.js RESTful API Services with beautiful code written in TypeScript",
  "main": "src/app.ts",
  "scripts": {
    "start": "nps",
    "test": "npm start test",
    "build": "npm start build",
    "presetup": "yarn install",
    "setup": "npm start config && npm start setup.script",
    "lint": "eslint '*/**/*.{js,ts,tsx}'",
    "docker:build": "docker build -t andreymashukov/ecommerce-backend:latest .",
    "docker:login": "echo $DOCKERHUB_PASSWORD | docker login -u $DOCKERHUB_USERNAME --password-stdin",
    "docker:push": "docker push andreymashukov/ecommerce-backend:latest",
    "ci:docker": "npm run docker:build && npm run docker:login && npm run docker:push"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "repository": "git+ssh://git@github.com/AndreMashukov/ecommerce-backend.git",
  "keywords": [
    "NodeJS",
    "TypeScript",
    "express",
    "ecommerce"
  ],
  "homepage": "https://github.com/AndreMashukov/ecommerce-backend#readme",
  "author": "Andrey Mashukov <andre.mashukov@gmail.com>",
  "dependencies": {
    "aws-sdk": "^2.721.0",
    "bcrypt": "5.0.0",
    "chalk": "^2.4.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.9.1",
    "class-validator-jsonschema": "^1.3.0",
    "commander": "^2.19.0",
    "compression": "^1.7.1",
    "copyfiles": "^2.1.0",
    "cors": "^2.8.4",
    "dataloader": "^1.3.0",
    "dotenv": "6.0.0",
    "event-dispatch": "^0.4.1",
    "express": "^4.17.3",
    "express-basic-auth": "^1.1.3",
    "express-status-monitor": "^1.0.1",
    "faker": "^4.1.0",
    "figlet": "^1.2.0",
    "glob": "^7.1.2",
    "helmet": "^3.9.0",
    "jsonfile": "5.0.0",
    "jsonwebtoken": "^8.5.1",
    "microframework-w3tec": "^0.6.3",
    "moment": "^2.24.0",
    "morgan": "^1.9.0",
    "mysql": "^2.16.0",
    "nodemon": "^1.12.1",
    "nps": "^5.9.3",
    "nps-utils": "^1.5.0",
    "pg": "^8.2.1",
    "pm2": "^4.4.0",
    "reflect-metadata": "^0.1.10",
    "routing-controllers": "^0.8.0",
    "routing-controllers-openapi": "^1.7.0",
    "serve-favicon": "^2.4.5",
    "supertest": "^3.0.0",
    "swagger-ui-express": "4.0.1",
    "ts-node": "7.0.1",
    "tslint": "^5.8.0",
    "typedi": "0.8.0",
    "typeorm": "^0.2.5",
    "typeorm-seeding": "1.0.0-beta.6",
    "typeorm-typedi-extensions": "^0.2.1",
    "typescript": "^3.6.3",
    "uuid": "^3.3.2",
    "winston": "3.1.0",
    "winston-cloudwatch": "^2.3.2"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/test/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testEnvironment": "node",
    "setupTestFrameworkScriptFile": "./test/unit/lib/setup.ts"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^2.0.0",
    "@types/bluebird": "^3.5.18",
    "@types/chalk": "^2.2.0",
    "@types/commander": "^2.11.0",
    "@types/cors": "^2.8.1",
    "@types/dotenv": "^4.0.2",
    "@types/express": "^4.0.39",
    "@types/faker": "^4.1.2",
    "@types/figlet": "^1.2.0",
    "@types/helmet": "0.0.41",
    "@types/jest": "23.3.2",
    "@types/morgan": "^1.7.35",
    "@types/nock": "^9.1.3",
    "@types/node": "^12.7.5",
    "@types/reflect-metadata": "0.1.0",
    "@types/serve-favicon": "^2.2.29",
    "@types/supertest": "^2.0.4",
    "@types/uuid": "^3.4.3",
    "@types/winston": "^2.3.7",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "cross-env": "^5.1.1",
    "eslint": "^7.3.1",
    "jest": "23.6.0",
    "mock-express-request": "^0.2.0",
    "mock-express-response": "^0.2.1",
    "ncp": "^2.0.0",
    "nock": "10.0.0",
    "ts-jest": "23.10.1"
  },
  "resolutions": {
    "**/event-stream": "^4.0.1"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    }
  }
}
